{
  "name": "reason-react-examples",
  "uncurried": true,
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "ppx-flags": [
    [
      "rescript-relay/ppx",
      "-uncurried"
    ]
  ],
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay",
    "@glennsl/rescript-fetch"
  ]
}